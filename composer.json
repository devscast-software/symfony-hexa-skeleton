{
    "name": "devscast/symfony-skeleton",
    "type": "project",
    "license": "CC-BY-NC-SA-4.0",
    "description": "A Devscast customize Symfony project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^2"
    },
    "flex-require": {
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/framework-bundle": "*",
        "symfony/runtime": "*",
        "symfony/yaml": "*",
        "symfony/rate-limiter": "*",
        "symfony/uid": "*",
	"symfony/locK": "*",

        "symfony/webapp-pack": "*",
        "league/glide": "^2.2.2",

        "symfony/ux-autocomplete": "^2.3",
        "symfony/ux-chartjs": "^2.3",
        "symfony/ux-cropperjs": "^2.3",
        "symfony/ux-dropzone": "^2.3",
        "symfony/ux-turbo": "^2.3",

        "beberlei/doctrineextensions": "^1.3",
        "endroid/qr-code-bundle": "^4.0",
        "erusev/parsedown": "^1.7",
        "knplabs/knp-paginator-bundle": "^5.9",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "scheb/2fa-backup-code": "^6.0",
        "scheb/2fa-bundle": "^6.0",
        "scheb/2fa-email": "^6.0",
        "scheb/2fa-google-authenticator": "^6.0",
        "scheb/2fa-trusted-device": "^6.0",
        
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/markdown-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "vich/uploader-bundle": "^1.21",

        "devscast/ddd-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/profiler-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/test-pack": "*",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/panther": "^2.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",

        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "hautelook/alice-bundle": "^2.10",
        "nelmio/alice": "^3.10",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.16",
        "squizlabs/php_codesniffer": "^3.6",
        "symplify/easy-coding-standard": "^10.1",
        "theofidry/alice-data-fixtures": "^1.5",
        "vimeo/psalm": "^4.21"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": false
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Fixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": false,
            "endpoint": [
                "https://raw.githubusercontent.com/devscast/symfony-repices/flex/main/index.json",
                "flex://defaults"
            ]
        }
    }
}
